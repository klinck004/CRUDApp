let mongoose = require('mongoose');

// create a model class
let schema = new mongoose.Schema({
    name: {type: String,},
    class:{type: String,},
    due: {type: String,},
    notes: {type: String,},
    mark: {type: Number,},
    weight: {type: Number,},
},
{
    collection:"assignment"
});

module.exports = mongoose.model('assignment', schema);
