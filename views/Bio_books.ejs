<% include partials/header.ejs %>
<% include partials/main_nav.ejs %>
<main class="container">
<p>should be text:  <%=test[3].name %></p>
    <div class="row">
        <div class="offset-md-1 col-md-10">
            <h1><% title %> </h1>        
        <br>
        <table class="table table-bordered table-striped table-hover">
            <tr class="d-flex">
                <th class="text-center col-2">Name</th>
                <th class="text-center col-2">Author</th>
                <th class="text-center col-1">Published</th>
                <th class="text-center col-4">Description</th>
                <th class="text-center col-1">Price</th>
            </tr>
            <% for(let count=0;count<BookList.length;count++) { %>    
            <tr class="d-flex">
                <td class="text-center col-2"><%=test[count].name %></td>
                <td class="text-center col-2"><%= test[count].author %></td>
                <td class="text-center col-1"><%= test[count].published %></td>
                <td class="text-center col-4"><%= test[count].description %></td>
                <td class="text-center col-1"><%= test[count].price %></td>
            </tr>
            <% } %>
        </table>
    </div>
    </div>
</main>
